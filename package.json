{
  "name": "underdog",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered sales training platform for cold calling practice",
  "scripts": {
    "dev": "pnpm --filter @underdog/web dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:integration": "turbo test:integration",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "next": "15.0.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "turbo": "^2.0.0",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
